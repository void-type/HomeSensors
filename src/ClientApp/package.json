{
  "name": "clientapp",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "watch": "vue-tsc --noEmit && vite build --watch --mode development",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier . --write",
    "generate-api": "sta -p https://localhost:5001/swagger/v1/swagger.json -o ./src/api/ --modular --responses --extract-request-params --disableStrictSSL"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@microsoft/signalr": "^6.0.9",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "chart.js": "^3.9.1",
    "chartjs-adapter-moment": "^1.0.0",
    "moment": "^2.29.4",
    "pinia": "^2.0.22",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/bootstrap": "^5.2.5",
    "@types/node": "^18.8.2",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "npm-check-updates": "^16.3.8",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "swagger-typescript-api": "^10.0.1",
    "typescript": "~4.8.4",
    "vite": "^3.1.4",
    "vue-tsc": "^0.40.13"
  },
  "overrides": {
    "swagger-typescript-api": {
      "typescript": "4.7.4"
    }
  }
}
